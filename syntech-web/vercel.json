{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api-python/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api-python/(.*)",
            "dest": "/api-python/main.py"
        }
    ]
}